type PublicUserInfo {
    id: UUID!
    userName: String!
}
enum GlobalUserRole {
    SUPER_USER,
    COURSE_CREATOR
}

type UserInfo {
    id: UUID!
    userName: String!
    firstName: String!
    lastName: String!
    nickname: String!
    realmRoles: [GlobalUserRole!]!
}

type AccessToken {
    accessToken: String!
    externalUserId: String
}

type ExternalUserIdWithUser {
    userId: UUID!
    externalUserId: String!
}